<?xml version="1.0"?>
<odoo>
        <record id="legal_m_queja_tree_view" model="ir.ui.view">
                <field name="name">legal_m.queja.tree.view</field>
                <field name="model">reporte.queja</field>
                <field name="arch" type="xml">
                        <tree string="Tree LQueja">
                                <field name="nombres"></field>
                                <field name="estado"></field>
                                <field name="email"></field>
                        </tree>
                </field>
        </record>

        <act_window id="action_legal_queja"
                name="Modulo Legal"
                res_model="reporte.queja"
                view_mode="tree,"
                />

        <menuitem id="legal_menu_root" name="Legal" sequence="1"></menuitem>
        <menuitem id="legal_queja_menu_category" name="Revision" sequence="1" parent="legal_menu_root"></menuitem>
        <menuitem id="action_legal_queja" name="Quejas" sequence="1" parent="legal_queja_menu_category" action="action_legal_queja"></menuitem>

</odoo>
